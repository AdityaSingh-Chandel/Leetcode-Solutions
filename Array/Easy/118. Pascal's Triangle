             # 16-01-25
class Solution:
    def generate(self, numRows: int) -> List[List[int]]:
        out=[]
        for i in range(numRows):
            l=[1]*(i+1)
            if out and len(out[-1])>=2:
                for j in range(1,i):
                    l[j]=out[-1][j]+out[-1][j-1]
            out.append(l)
        return out
